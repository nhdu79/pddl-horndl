(define (problem elevators_problem)
	(:domain elevators)
	(:objects pass_r pass_u pass_x pass_n pass_w pass_g pass_k pass_y pass_d pass_l a b c e f h i j m o p q s t v )
	(:init
		(origin pass_r b)
		(destin pass_r t)
		(origin pass_u v)
		(destin pass_u a)
		(origin pass_x v)
		(destin pass_x b)
		(origin pass_n o)
		(destin pass_n v)
		(origin pass_w t)
		(destin pass_w h)
		(origin pass_g f)
		(destin pass_g i)
		(origin pass_k c)
		(destin pass_k s)
		(origin pass_y p)
		(destin pass_y t)
		(origin pass_d p)
		(destin pass_d a)
		(origin pass_l o)
		(destin pass_l o)
		(next a b)
		(next b c)
		(next c e)
		(next e f)
		(next f h)
		(next h i)
		(next i j)
		(next j m)
		(next m o)
		(next o p)
		(next p q)
		(next q s)
		(next s t)
		(next t v)
		(lift_at a)
	)
	(:goal (not (exists (?x) 
 		 (and 
 			 (mko (passenger ?x)) 
 			 (not (mko (served ?x))) 
 		 )) 
 	 ))
)