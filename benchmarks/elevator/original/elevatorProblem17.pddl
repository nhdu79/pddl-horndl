(define (problem elevators_problem)
	(:domain elevators)
	(:objects pass_j pass_o pass_k pass_i pass_h pass_n a b c d e f g l m p q )
	(:init
		(origin pass_j m)
		(destin pass_j p)
		(origin pass_o p)
		(destin pass_o l)
		(origin pass_k p)
		(destin pass_k l)
		(origin pass_i f)
		(destin pass_i l)
		(origin pass_h d)
		(destin pass_h m)
		(origin pass_n f)
		(destin pass_n g)
		(next a b)
		(next b c)
		(next c d)
		(next d e)
		(next e f)
		(next f g)
		(next g l)
		(next l m)
		(next m p)
		(next p q)
		(lift_at b)
	)
	(:goal (not (exists (?x) 
 		 (and 
 			 (mko (passenger ?x)) 
 			 (not (mko (served ?x))) 
 		 )) 
 	 ))
)